String split(String str, int indx, char c){
  String strs[20];
  int StringCount = 0;
  while (str.length() > 0)
  {
    int index = str.indexOf(c);
    if (index == -1) // No space found
    {
      strs[StringCount++] = str;
      break;
    }
    else
    {
      strs[StringCount++] = str.substring(0, index);
      str = str.substring(index+1);
    }
  }
  for (int i = 0; i < StringCount; i++)
  {
    if (indx == i){
      return strs[i];  
    }
  }
 return " ";
}
