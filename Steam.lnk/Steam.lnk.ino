void callback(char* topic, byte* payload, unsigned int length) {
 
  Serial.print("Message arrived in topic: ");
  Serial.println(topic);
  Serial.print("Message:");

  if ((String)topic == "topicCryptoCurrency")
  {
    crypto = "";
    for (int i = 0; i < length; i++) {
      crypto = crypto + (char)payload[i];
    }
    Serial.print(crypto);
  }
  if ((String)topic == "topicMatch")
  {
    matchOfWeek = "";
    for (int i = 0; i < length; i++) {
      matchOfWeek = matchOfWeek + (char)payload[i];
    }
    Serial.print(matchOfWeek);
  }
  if ((String)topic == "topicTopThree")
  {
    topThree = "";
    for (int i = 0; i < length; i++) {
      topThree = topThree + (char)payload[i];
    }
    Serial.print(topThree);
  }
   
  if ((String)topic == "topicWeather")
  {
    weather = "";
    for (int i = 0; i < length; i++) {
      weather = weather + (char)payload[i];
    }
    Serial.print(weather);
  }

  Serial.println();
  Serial.println("-----------------------");
 
}

void reconnect() {
  while (!client.connected()) {

    Serial.print("Attempting MQTT connection...");
    String clientId = "ESP8266Client-";
    clientId += String(random(0xffff), HEX);
    if (client.connect(clientId.c_str())) {

      Serial.println("connected");

    } else {

      Serial.print("failed, rc=");
      Serial.print(client.state());
      Serial.println(" try again in 5 seconds");
      delay(5000);

    }
      client.subscribe("topicMatch");
      client.subscribe("topicTopThree");
      client.subscribe("topicWeather");
      client.subscribe("topicCryptoCurrency");

  }
}


